# Copy this file to `config.properties` and update values to match your MySQL setup
# Location: project root (same folder as pom.xml)

# JDBC URL (database must exist)
# Example: jdbc:mysql://localhost:3306/event_club_db
db.url=jdbc:mysql://localhost:3306/event_club_db

# Database user and password
# Replace with your MySQL username and password
db.user=root
db.password=YOUR_MYSQL_PASSWORD
